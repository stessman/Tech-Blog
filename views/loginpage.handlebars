<div class="add-user-container">
  <h2>
    RSVP with the dish you plan to bring!
  </h2>
  <form class="new-user-form">
    <label for="user_name_field">
      Username:
    </label>
    <br />
    <input id="user_name_field" name="username field">
      {{user_name}}
    </input>
    <br />
    <label for="email_field">
      Email:
    </label>
    <br />
    <input id="email_field" name="email field">
      {{email}}
    </input>
    <br />
    <label for="password_field">
      Password:
    </label>
    <br />
    <input id="password_field" name="password field">
      {{password}}
    </input>
    <br />
    <button type="submit" value="Submit">
      Submit
    </button>
  </form>
</div>
<script src="/js/loginpage.js"></script>